{
  "schema_version": 1,
  "quilt_loader": {
    "group": "com.diontryban.ash",
    "id": "ash",
    "version": "${version}",
    "metadata": {
      "name": "Ash API",
      "description": "An API that provides an abstraction layer between the three main Minecraft mod loaders (Fabric, Forge, and Quilt).",
      "contributors": {
        "Trikzon": "Owner"
      },
      "contact": {
        "homepage": "https://github.com/Trikzon/ash",
        "issues": "https://github.com/Trikzon/ash/issues",
        "sources": "https://github.com/Trikzon/ash"
      },
      "icon": "assets/ash/icon.png",
      "license": "LGPL-3.0-or-later"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "modmenu": "com.diontryban.ash.impl.client.gui.screens.ModOptionsScreenRegistryImpl"
    },
    "depends": [
      {
        "id": "quilt_loader",
        "versions": ">=0.17.6-"
      },
      {
        "id": "quilted_fabric_api",
        "versions": ">=5.0.0-"
      },
      {
        "id": "minecraft",
        "versions": ">=1.19.3"
      }
    ]
  },
  "mixin": [
    "ash.mixins.json",
    "ash.quilt.mixins.json"
  ],
  "modmenu": {
    "badges": [ "library" ]
  }
}
